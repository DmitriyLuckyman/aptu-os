C-FLAGS = -Wall -Werror
all: allocator
main.o: main.c
	gcc $(C-FLAGS) -c main.c -o main.o
heap_functions.o: 
requests.o:
shell.o: shell.c
	gcc $(C-FLAGS) -c shell.c -o shell.o
allocator: main.o heap_functions.o requests.o
	gcc $(C-FLAGS) main.o heap_functions.o requests.o -o allocator
clean:
	rm -rf allocator
